<h2>Photo Gallery of <%= @current_user.name %></h2>

<ul>
<% if @current_user.photos.present? %>
    <% @current_user.photos.each do |photo| %>
    <li>
        <%= image_tag photo.url, alt: @current_user.name %>
        <% if photo.user_id == @current_user.id %>
            (<%= link_to 'Delete', photo_path( photo.id ), method: 'delete' %>)
        <% end %>
    </li>
    <% end %>
<% end %>
</ul>